#include 'protheus.ch'

user function estrutur()
    
    Local nNum1 := 22
    Local nNum2 := 100

    if (nNum1 = nNum2)
        MsgAlert("A variável nNum1 é igual a nNum2")
        ElseIf (nNum1 > nNum2)
            MsgAlert("A variável é maior")
        Elseif (nNum1 != nNum2)
            Alert ("A variável nNum1 é diferente de nNum2")
            EndIf
return 
